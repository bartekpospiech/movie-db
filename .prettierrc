{
  "$schema": "https://json.schemastore.org/prettierrc",
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "**/*.svg",
      "options": {
        "parser": "markdown"
      }
    },
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
